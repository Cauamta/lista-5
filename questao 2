#include <stdio.h>
#include <stdlib.h>
/*
Aluno:cauã mata
Matricula:2412130053
*/

int main() {
    char input[100];
    int length = 0;

    printf("Digite uma string: ");
    scanf("%s", input);

    while (input[length] != '\0') {
        length++;
    }

    printf("Número de caracteres: %d\n", length);

    return 0;
}
